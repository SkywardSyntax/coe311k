   "source": [
    "import numpy as np\n",
    "\n",
    "base10 = int(input(\"Input a base 10 number\"))\n",
    "diglist = np.array([])\n",
    "\n",
    "while base10 > 0:\n",
    "    diglist = np.append(diglist, int(base10%2))\n",
    "    base10 = base10//2\n",
    "\n",
    "print(diglist[-1::-1])"
   ]
  }
